<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.recyclerview">
    <permission android:name="android.permission.INTERNET"/>

    <application
        android:allowBackup="true"
        android:icon="@drawable/po_english_icon"
        android:label="@string/app_name"
        android:roundIcon="@drawable/po_english_icon"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".activity.SettingActivity"></activity>
        <activity android:name=".activity.WordDetailActivity" />
        <activity android:name=".activity.FastLearningActivity" />
        <activity android:name=".activity.WordListActivity" />
        <activity android:name=".activity.LearningAndPracticeActivity" />
        <activity android:name=".activity.TopicActivity" />
        <activity android:name=".activity.RemindActivity" />
        <activity android:name=".activity.MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <receiver android:name=".notification.NotificationReceiver" >
        </receiver>
    </application>

</manifest>